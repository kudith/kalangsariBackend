{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "singularName": "event",
    "pluralName": "events",
    "displayName": "Event",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "event_name": {
      "type": "string"
    },
    "date": {
      "type": "date"
    },
    "location": {
      "type": "string",
      "required": true
    },
    "time": {
      "type": "time"
    },
    "description": {
      "type": "text"
    },
    "image": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": true
    },
    "volunteers": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::volunteer.volunteer",
      "inversedBy": "events"
    }
  }
}
